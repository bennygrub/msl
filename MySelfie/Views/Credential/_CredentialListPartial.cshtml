@model MySelfie.Models.CredentialListViewModel
	
<table id="dt_wall" class="table table-striped table-bordered table-hover">
	<thead>
		<tr>
			<th>User Name</th>
			<th>App Name</th>
			<th>Usage Today</th>
			<th>Modify</th>
            <th>Delete</th>
		</tr>
	</thead>
	<tbody>
@foreach (var item in Model.CredentialList) {
        <tr id="row_@item.CredentialId" data-credential-id="@item.CredentialId" data-credential-name="@item.UserName">
            <td>
                @item.UserName
            </td>
            <td>
                @item.AppName
            </td>
            <td>
                @item.UsageToday
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.CredentialId })
            </td>
            <td>
                <i class="btn remove-credential-btn fa fa-lg fa-fw fa-times-circle" rel="tooltip" data-placement="bottom" data-original-title="Remove Credential"></i>
            </td>
        </tr>
}												
	</tbody>
</table>				